<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cat.admin.modules.mapper.AccountMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.cat.admin.modules.entity.AccountEntity">
        <id column="id" property="id" />
        <result column="email" property="email" />
        <result column="name" property="name" />
        <result column="password" property="password" />
        <result column="register_time" property="registerTime" />
        <result column="login_time" property="loginTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, email, name, password, register_time, login_time
    </sql>

    <select id="selectAccount" resultType="java.util.Map">
        select id, email, name, password, register_time, login_time from account
    </select>

    <select id="selectAccount2" resultType="java.util.Map">
        select id, email, name, password, register_time, login_time from account
    </select>
</mapper>
